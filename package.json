{
  "name": "@wharfie/wharfie",
  "version": "0.0.6-1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wharfie/wharfie.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wharfie/wharfie/issues"
  },
  "bin": {
    "wharfie": "./bin/wharfie"
  },
  "homepage": "https://github.com/wharfie/wharfie#readme",
  "engineStrict": true,
  "engines": {
    "node": ">=20",
    "npm": ">=10.2"
  },
  "main": "./client/",
  "scripts": {
    "grammar:generate": "antlr -Dlanguage=JavaScript lambdas/lib/athena/grammar/athenasql.g4 -o lambdas/lib/athena/antlr-lib -Xexact-output-dir",
    "typecheck": "tsc",
    "test:js": "TZ=UTC jest --silent --coverage",
    "test:update": "TZ=UTC jest --silent --updateSnapshot",
    "test": "npm run test:js",
    "test:ci": "npm run lint && npm run typecheck && npm run test",
    "lint:js": "eslint . && prettier --check \"{*,**/*}.{js,json}\"",
    "lint": "npm run lint:js",
    "lint:fix": "eslint --fix . && prettier --write \"{*,**/*}.{js,json}\""
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always"
  },
  "jest": {
    "collectCoverageFrom": [
      "lambdas/**/*.js",
      "!lambdas/lib/athena/antlr-lib/*.js"
    ],
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "env": {
      "es2020": true,
      "node": true
    },
    "plugins": [
      "jsdoc"
    ],
    "ignorePatterns": [
      "antlr-lib/",
      "dist/"
    ],
    "overrides": [
      {
        "files": [
          "test/**"
        ],
        "plugins": [
          "jest"
        ],
        "extends": [
          "plugin:jest/all"
        ],
        "rules": {
          "jest/max-expects": 0,
          "jest/no-conditional-in-test": 0
        }
      }
    ],
    "extends": [
      "plugin:jsdoc/recommended",
      "plugin:node/recommended",
      "eslint-config-standard",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "settings": {
      "jsdoc": {
        "mode": "typescript"
      }
    },
    "rules": {
      "jsdoc/check-types": 0,
      "camelcase": 0,
      "no-template-curly-in-string": 0
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write \"{*,**/*}.{js,json}\"",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/antlr4": "^4.7.2",
    "@types/aws-lambda": "^8.10.71",
    "@types/bluebird": "^3.5.36",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.18.48",
    "@types/uuid": "^8.3.0",
    "aws-sdk-client-mock": "^3.0.0",
    "aws-sdk-client-mock-jest": "^3.0.0",
    "esbuild": "^0.17.16",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsdoc": "^44.0.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^3.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^9.4.3",
    "nock": "^13.1.3",
    "prettier": "^2.8.8",
    "traverse": "^0.6.7",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-athena": "^3.363.0",
    "@aws-sdk/client-cloudformation": "^3.363.0",
    "@aws-sdk/client-cloudwatch": "^3.363.0",
    "@aws-sdk/client-dynamodb": "^3.363.0",
    "@aws-sdk/client-firehose": "^3.441.0",
    "@aws-sdk/client-glue": "^3.363.0",
    "@aws-sdk/client-s3": "^3.363.0",
    "@aws-sdk/client-sns": "^3.363.0",
    "@aws-sdk/client-sqs": "^3.363.0",
    "@aws-sdk/client-sts": "^3.363.0",
    "@aws-sdk/credential-providers": "^3.363.0",
    "@aws-sdk/lib-dynamodb": "^3.365.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sandfox/arn": "^2.1.0",
    "antlr4": "4.9.2",
    "apache-arrow": "^12.0.1",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.0",
    "cli-progress": "^3.9.0",
    "death": "^1.1.0",
    "esbuild": "^0.17.16",
    "inquirer": "^8.2.0",
    "joi": "^17.9.1",
    "jszip": "^3.10.1",
    "open": "^7.4.2",
    "yargonaut": "^1.1.4",
    "yargs": "^17.1.1"
  }
}
