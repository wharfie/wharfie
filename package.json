{
  "name": "@wharfie/wharfie",
  "version": "0.0.14",
  "description": "",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wharfie/wharfie.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wharfie/wharfie/issues"
  },
  "bin": {
    "wharfie": "./bin/wharfie"
  },
  "homepage": "https://github.com/wharfie/wharfie#readme",
  "engineStrict": true,
  "engines": {
    "node": "24.12.0",
    "npm": "11.7.0"
  },
  "main": "./client/",
  "scripts": {
    "typecheck": "tsc",
    "test:js": "TZ=UTC node --experimental-vm-modules node_modules/jest/bin/jest.js --silent --coverage",
    "test:update": "TZ=UTC node --experimental-vm-modules node_modules/jest/bin/jest.js --silent --updateSnapshot",
    "test": "npm run test:js",
    "test:ci": "npm run lint && npm run typecheck && npm run test",
    "lint:js": "eslint . && prettier --check \"{*,**/*}.{js,json}\"",
    "lint": "npm run lint:js",
    "lint:fix": "eslint --fix . && prettier --write \"{*,**/*}.{js,json}\"",
    "test:watch": "TZ=UTC node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:watchAll": "TZ=UTC node --experimental-vm-modules node_modules/jest/bin/jest.js --watchAll"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always"
  },
  "jest": {
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [
      ".js"
    ],
    "transform": {},
    "testMatch": [
      "<rootDir>/test/actor/runtime/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "lambdas/lib/actor/runtime/**/*.js",
      "lambdas/lib/actor/resources/builds/**/*.js",
      "lambdas/lib/actor/resources/base-resource.js",
      "lambdas/lib/actor/resources/base-resource-group.js",
      "lambdas/lib/actor/resources/reconcilable.js",
      "lambdas/lib/code-execution/**/*.js",
      "lambdas/lib/db/**/*.js",
      "lambdas/lib/queue/base.js",
      "lambdas/lib/object-storage/base.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/mock/builds/"
    ]
  },
  "eslintConfig": {
    "env": {
      "es2020": true,
      "node": true
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "ecmaVersion": "latest",
      "sourceType": "module",
      "babelOptions": {
        "plugins": [
          "@babel/plugin-syntax-import-attributes"
        ]
      }
    },
    "plugins": [
      "jsdoc"
    ],
    "ignorePatterns": [
      "dist/",
      "tmp/",
      "docs/",
      "cli/",
      "test/",
      "scratch/",
      "coverage/"
    ],
    "overrides": [
      {
        "files": [
          "test/**"
        ],
        "plugins": [
          "jest"
        ],
        "extends": [
          "plugin:jest/all"
        ],
        "rules": {
          "jest/max-expects": 0,
          "jest/no-conditional-in-test": 0
        }
      }
    ],
    "extends": [
      "plugin:jsdoc/recommended",
      "plugin:node/recommended",
      "plugin:import/recommended",
      "plugin:import/typescript",
      "eslint-config-standard",
      "prettier"
    ],
    "settings": {
      "jsdoc": {
        "mode": "typescript"
      },
      "import/resolver": {
        "typescript": {
          "project": "./tsconfig.json"
        },
        "node": {
          "extensions": [
            ".js",
            ".ts",
            ".d.ts"
          ]
        }
      }
    },
    "rules": {
      "import/no-dynamic-require": "error",
      "jsdoc/check-types": "off",
      "camelcase": "off",
      "no-template-curly-in-string": "off",
      "jsdoc/no-undefined-types": [
        "error",
        {
          "definedTypes": [
            "Iterable",
            "AsyncIterable"
          ]
        }
      ]
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write \"{*,**/*}.{js,json}\"",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-syntax-import-attributes": "^7.27.1",
    "@jest/globals": "^30.2.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/bluebird": "^3.5.36",
    "@types/cli-progress": "^3.11.6",
    "@types/diff": "^5.2.1",
    "@types/inquirer": "^8.0.7",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.0",
    "@types/npmcli__arborist": "^6.3.1",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/parser": "^8.46.3",
    "@typescript-eslint/utils": "^8.49.0",
    "aws-sdk-client-mock": "^3.0.0",
    "aws-sdk-client-mock-jest": "^3.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^29.2.1",
    "eslint-plugin-jsdoc": "^44.0.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^3.1.0",
    "jest": "^29.7.0",
    "lint-staged": "^9.4.3",
    "nock": "^13.1.3",
    "prettier": "^3.7.4",
    "traverse": "^0.6.7",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-application-auto-scaling": "^3.556.0",
    "@aws-sdk/client-athena": "^3.363.0",
    "@aws-sdk/client-cloudformation": "^3.363.0",
    "@aws-sdk/client-cloudwatch": "^3.363.0",
    "@aws-sdk/client-cloudwatch-events": "^3.554.0",
    "@aws-sdk/client-dynamodb": "^3.363.0",
    "@aws-sdk/client-firehose": "^3.540.0",
    "@aws-sdk/client-glue": "^3.363.0",
    "@aws-sdk/client-iam": "^3.540.0",
    "@aws-sdk/client-lambda": "^3.536.0",
    "@aws-sdk/client-s3": "^3.363.0",
    "@aws-sdk/client-sns": "^3.363.0",
    "@aws-sdk/client-sqs": "^3.363.0",
    "@aws-sdk/client-sts": "^3.363.0",
    "@aws-sdk/credential-provider-node": "^3.734.0",
    "@aws-sdk/credential-providers": "^3.363.0",
    "@aws-sdk/lib-dynamodb": "^3.365.0",
    "@aws-sdk/s3-request-presigner": "^3.910.0",
    "@aws-sdk/util-dynamodb": "^3.658.1",
    "@duckdb/node-api": "^1.4.2-r.1",
    "@npmcli/arborist": "^9.1.4",
    "@paralleldrive/cuid2": "^2.2.2",
    "@smithy/util-retry": "^3.0.3",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "apache-arrow": "^12.0.1",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "boxednode": "^2.4.4",
    "bpkg": "^0.9.2",
    "chalk": "^4.1.0",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "corestore": "^7.4.7",
    "death": "^1.1.0",
    "detect-libc": "^2.1.2",
    "diff": "^5.2.0",
    "env-paths": "^2.2.1",
    "es-toolkit": "^1.13.1",
    "esbuild": "0.25.1",
    "flatted": "^3.3.1",
    "hyparquet": "^1.8.4",
    "hyparquet-compressors": "^1.0.0",
    "hyperbee": "^2.26.5",
    "hypercore": "^11.16.1",
    "hyperdht": "^6.23.0",
    "hyperswarm": "^4.15.1",
    "inquirer": "^8.2.0",
    "joi": "^17.9.1",
    "js-yaml": "^4.1.0",
    "jsondiffpatch": "^0.5.0",
    "jszip": "^3.10.1",
    "lexe": "^0.1.0-alpha.6",
    "lmdb": "^3.4.0",
    "node-sql-parser": "^5.3.0",
    "node-ssh": "^13.2.1",
    "package-up": "^5.0.0",
    "pacote": "^21.0.3",
    "parcel": "^2.14.4",
    "pkg-up": "^3.1.0",
    "postject": "^1.0.0-alpha.6",
    "semver": "^7.6.3",
    "source-map-support": "^0.5.21",
    "sshpk": "^1.18.0",
    "tar": "^7.4.3",
    "uuid": "^11.1.0"
  }
}
